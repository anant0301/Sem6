   "source": [
    "df = pd.read_csv('sample_data_1.csv')\n",
    "tally = []\n",
    "for frequency in df.Frequency:\n",
    "    ones = '|' * (frequency % 5)\n",
    "    fives= '||||\\ ' * (frequency // 5)\n",
    "    tally.append(fives + ' ' + ones)\n",
    "df['Tally'] = tally\n",
    "df[['Score', 'Tally', 'Frequency']]"
   ]
